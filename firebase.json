{
  "hosting": {
    "rewrites": [
      {
        "source": "/products.html",
        "destination": "/products.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
